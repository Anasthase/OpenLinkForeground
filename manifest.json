{
    "manifest_version": 2,
    "name": "Open Link Foreground",
    "description": "Add a context menu option to open links in a new foreground tab.",
    "version": "1.0",
    "homepage_url": "https://github.com/Anasthase/OpenLinkForeground",

    "background": {
        "scripts": [
            "background.js"
        ]
    },

    "permissions": [        
        "contextMenus"
    ]
}